<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="customer_service">
  <select id="getNotice" resultType="mybatis.vo.NoticeVO">
    SELECT * FROM notice_table
    WHERE status = 0
    ORDER BY idx desc
  </select>

  <select id="getFaq" resultType="mybatis.vo.FAQVO">
    SELECT * FROM faq_table
    WHERE status = 0
    ORDER BY idx desc
  </select>
</mapper>